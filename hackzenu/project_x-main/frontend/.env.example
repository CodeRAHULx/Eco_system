# ============================================================================
# SafeRoute AI - Frontend Environment Configuration
# ============================================================================

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_AI_URL=http://localhost:8000
REACT_APP_SOCKET_URL=http://localhost:5000

# Map Configuration
REACT_APP_MAPBOX_TOKEN=your_mapbox_token
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_key

# Environment
NODE_ENV=development
REACT_APP_ENV=development

# Debug Mode
REACT_APP_DEBUG=false

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_LOCATION_TRACKING=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# Analytics
REACT_APP_ANALYTICS_ID=your_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Defaults
REACT_APP_DEFAULT_ALERT_RADIUS=10
REACT_APP_DEFAULT_MAP_ZOOM=13
